<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jennifer.jennifer">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.BLUETOOTH" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:name=".application.JenniferApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".ui.main.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.parallax.GuideActivity"
            android:label="@string/title_activity_guide"
            android:screenOrientation="portrait" />
        <activity
            android:name=".base.BaseActivity"
            android:label="@string/title_activity_base" />
        <activity
            android:name=".ui.web.WebViewActivity"
            android:label="@string/title_activity_guide"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.palette.PaletteActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".ui.environment.EnvironmentTestActivity" />
        <activity android:name=".ui.trtc.TRTCVideoTestActivity" />
        <activity android:name=".ui.life.LifeCycleActivity" />
        <activity android:name=".ui.life.LifeCycle2Activity" />
        <activity android:name=".ui.radio.RecycleViewRadioButtonActivity"></activity>
    </application>

</manifest>