<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl_recommend"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ui.parallax.fragment.GuideRecommendFragment">

    <ImageView
        android:id="@+id/iv_recommend_layer4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/guide_recommend_layer4" />

    <ImageView
        android:id="@+id/iv_recommend_layer1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/guide_recommend_layer1" />

    <ImageView
        android:id="@+id/iv_recommend_layer2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/guide_recommend_layer2" />

    <ImageView
        android:id="@+id/iv_recommend_layer3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/guide_recommend_layer3" />

</RelativeLayout>