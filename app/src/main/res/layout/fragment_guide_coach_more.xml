<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl_coach_more"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ui.parallax.fragment.GuideCoachMoreFragment">

    <!-- TODO: Update blank fragment layout -->
    <ImageView
        android:id="@+id/iv_coach_more_layer4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/guide_coach_more_layer4" />

    <ImageView
        android:id="@+id/iv_coach_more_layer1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/guide_coach_more_layer1" />

    <ImageView
        android:id="@+id/iv_coach_more_layer2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/guide_coach_more_layer2" />

    <ImageView
        android:id="@+id/iv_coach_more_layer3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/guide_coach_more_layer3" />

    <TextView
        android:id="@+id/tv_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="48dp"
        android:background="@drawable/shape_radiu10_base"
        android:paddingBottom="10dp"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:paddingTop="10dp"
        android:text="@string/guide_start"
        android:textColor="@color/base_color"
        android:textSize="14sp"
        android:visibility="visible" />

</RelativeLayout>